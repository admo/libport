#ifndef LIBPORT_CERRNO
# define LIBPORT_CERRNO

# include <libport/detect-win32.h>

# include <cerrno>

# if defined WIN32
// Boost itself redefines ETIMEDOUT. Do not do it if Boost did.
#  ifndef ETIMEDOUT
#   define ETIMEDOUT 110
#  endif

#  define ENOTSUP 111
# endif

#endif // ! LIBPORT_CERRNO

