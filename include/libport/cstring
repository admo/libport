#ifndef LIBPORT_CSTRING
# define LIBPORT_CSTRING

# include <cstring>

/// Whether the C string \a Lhs and \a Rhs are equal.
# define STREQ(Lhs, Rhs) (!strcmp (Lhs, Rhs))

